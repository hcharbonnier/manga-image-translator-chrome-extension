{
    "manifest_version": 3,
    "name": "manga-image-translator-chrome-extension",
    "version": "1.0",
    "action": {
      "default_popup": "options.html"
    },
    "permissions": ["storage", "activeTab", "scripting","tabs"],
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": [ "http://localhost/*", "*://*/*", "\u003Call_urls>" ]
  }